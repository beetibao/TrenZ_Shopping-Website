<div id="products_items">
        <div class="product_title">
            <h2>Trang chủ</h2>
            <h1>Tất cả sản phẩm cho Nữ giới</h1>
        </div>
    </div>
    <!-- filter and category-->
    <section class="filter">
        <div class="container">
            <div class="product_row">
                <div class="filter_left">
                    <div class="filter-section">
                        <h3>Khoảng giá (VNĐ)</h3>
                        <label class="container1">Nhỏ hơn 100.000đ
                            <input 
                                type="radio" 
                                onchange="handleChangePriceRange(this, 0, 100)" 
                                name="price" 
                                value="0,100"
                                onclick="handleUnSelectPrice(this)"
                            >
                            <span class="checkmark"></span>
                        </label>
                        <label class="container1">Từ 100.000đ - 200.000đ
                            <input onclick="handleUnSelectPrice(this)" type="radio" onchange="handleChangePriceRange(this, 100, 200)"
                                name="price" value="100,200">
                            <span class="checkmark"></span>
                        </label>
                        <label class="container1">Từ 200.000đ - 350.000đ
                            <input onclick="handleUnSelectPrice(this)" type="radio" onchange="handleChangePriceRange(this, 200, 350)"
                                name="price" value="200,350">
                            <span class="checkmark"></span>
                        </label>
                        <label class="container1">Từ 350.000đ - 600.000đ
                            <input onclick="handleUnSelectPrice(this)" type="radio" onchange="handleChangePriceRange(this, 350, 600)"
                                name="price" value="350,600">
                            <span class="checkmark"></span>
                        </label>
                        <label class="container1">Lớn hơn 600.000đ
                            <input onclick="handleUnSelectPrice(this)" type="radio" onchange="handleChangePriceRange(this, 600)" name="price"
                                value="600,">
                            <span class="checkmark"></span>
                        </label>
                          
                        
                       
                    </div>
                    <div class="filter-section">
                        <h3>Kích thước</h3>
                        <label class="container1">S
                            <input onclick="handleUnSelectSize(this)" onchange="handleChangeSize(this)" type="radio" name="size" value="S">
                            <span class="checkmark"></span>
                        </label>
                        <label class="container1">M
                            <input onclick="handleUnSelectSize(this)" onchange="handleChangeSize(this)" type="radio" name="size" value="M">
                            <span class="checkmark"></span>
                        </label>
                        <label class="container1">L
                            <input onclick="handleUnSelectSize(this)" onchange="handleChangeSize(this)" type="radio" name="size" value="L">
                            <span class="checkmark"></span>
                        </label>
                        <label class="container1">XL
                            <input onclick="handleUnSelectSize(this)" onchange="handleChangeSize(this)" type="radio" name="size" value="XL">
                            <span class="checkmark"></span>
                        </label>
                        <label class="container1">XXL
                            <input onclick="handleUnSelectSize(this)" onchange="handleChangeSize(this)" type="radio" name="size" value="XXL">
                            <span class="checkmark"></span>
                        </label>
                    </div>
                </div>

                <div class="filter_right">
                    <div class="filter_right_items">
                        <label for="category">Sắp xếp theo</label>
                        <select id="category">
                            <option value="all_clothing">Tất cả</option>
                            <option value="ex_clothing">Đắt nhất</option>
                            <option value="cheap_clothing">Rẻ nhất</option>
                            <option value="latest_clothing">Mới nhất</option>
                        </select>     
                    </div>

                    <div class="products_content row">   
                    {{#each products}}
                          <div class="products_content_items">
                            <div class="products_content_list">
                                <a href="/womenProduct/{{this.id}}">
                                <img class="detail_product" src="{{this.img}}" alt="{{this.name}}"  >
                            </a>

                            <a href="/womenProduct/{{this.id}}">
                                <p class="description">{{this.name}}</p>                              
                            </a>
                            <p class="price">{{this.price}}đ</p>
                            </div>                         
                        </div>
                        {{/each}}
                    </div>

                    <div class="filter_right_numbers ">
                        <div class="pagination">
                            <ul>
                              <li><span class="pagination-arrow">&#171</span></li>
                              <li onclick="handlePaginate(1)" id="page1" class="pagination-item">1</li>
                              <li onclick="handlePaginate(2)" id="page2" class="pagination-item">2</li>
                              <li onclick="handlePaginate(3)" id="page3" class="pagination-item">3</li>
                              <li><span class="pagination-arrow">&#187</span></li>                       
                            </ul>
                          </div>
                    </div>
                   
                </div>
                
            </div>
            
        </div>
    </section>